{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "server",
  "projectType": "application",
  "sourceRoot": "apps/server/src",
  "implicitDependencies": ["core", "integrations", "instructions", "shared"],
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/server",
        "command": "pnpm run dev"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/server",
        "command": "pnpm run start"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/apps/server/dist/runtime"],
      "options": {
        "cwd": "apps/server",
        "command": "pnpm run build"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/server",
        "command": "pnpm run typecheck"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/server",
        "command": "pnpm run lint"
      }
    }
  }
}
